<div>
  <div>Posts</div>
  <div>
    <% posts.forEach(post => { %>
    <div>
      <p><%= post.title %></p>
      <p><%= post.formatted_date %></p>
      <p><%= post.username %></p>
      <p><%= post.contents %></p>
      <form action="/delete/<%= post.post_id %>" method="POST">
        <button type="submit">Delete post</button>
      </form>
    </div>
    <% }); %>
  </div>
</div>
